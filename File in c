#include<stdio.h>
int main()
{
int id,n,i;
char name[20],add[20];
FILE*fp;
 fp=open("student.txt","w");
printf("enter the number of student");
 scanf("%d",&n);
 printf("enter id,name,address of the student");
 for (i=1;i<=n;i++)
 {
  scanf("%d %s %s",&id,name,add);
  fprintf(fp,"%d %s %s",&id,name,add);
 
}
fclose(fp);
return 0;
}
